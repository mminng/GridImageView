<?xml version="1.0" encoding="utf-8"?>
<com.github.mminng.gridimage.widget.SquareRoundFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_edit_grid_add_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/item_edit_grid_add_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F1F1F1" />

    <ImageView
        android:id="@+id/item_edit_grid_add_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_grid_add"
        tools:ignore="ContentDescription" />

</com.github.mminng.gridimage.widget.SquareRoundFrameLayout>